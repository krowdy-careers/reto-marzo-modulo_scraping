{
  "name": "tottus-scraping",
  "version": "1.0.0",
  "description": "TypeScript-based web scraper using Puppeteer for data extraction from Tottus",
  "license": "MIT",
  "author": "Arnoldo",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "build": "tsc --build",
    "start": "node --env-file=.env dist/app.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "dependencies": {
    "class-validator": "^0.14.1",
    "csv-writer": "^1.6.0",
    "openai": "^4.89.0",
    "puppeteer": "^24.4.0"
  }
}
